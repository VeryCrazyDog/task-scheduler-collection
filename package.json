	"engines": {
		"node": ">= 12.x"
	},
	"dependencies": {},
	"devDependencies": {
		"@types/node": "^12.20.13",
		"@typescript-eslint/eslint-plugin": "4.x",
		"ava": "3.x",
		"eslint": "7.x",
		"eslint-config-standard-with-typescript": "20.x",
		"eslint-plugin-import": "2.x",
